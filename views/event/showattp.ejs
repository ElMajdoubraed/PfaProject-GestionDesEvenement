<% if((user)&&(user.admin=="true")) { %>
    <%- include('../partials/header') -%>
    <%- include('../partials/navbar') -%>
    <div class="container">
    <div class="mt-5">
      <% if ((user) && (user.admin== "true")) {%>
        <div class="mb-3">
            <a href="<%=  '/events/EventAttribut/' + event.event_id %>"><h3 class="text-info">Back to the event attribut</h3></a>
        </div>
        <%}%>
    
        <div class="">
            <div class="card">
                        
                <div class="card-body">
                    <h5 class="card-title"><%=event.type %></h5>
                    <hr>
                    <%if (event.status=="Open")  {%>
                        <h5 class="text-success"> <%=event.status%></h5>
                        <%} else { %>
                          <h5 class="text-danger"> <%=event.status%></h5>
                          <%}%>
                          <p><b>Course : </b><%=event.course%></p>
                    <%if ((event.remarque==null)||(event.remarque==""))  {%>
                        <%} else { %>
                          <p><b>Notes : </b> <%=event.remarque%></p>
                          <%}%>
                    <% if ((user) && (user.admin== "true")) {%>
                        <a href="<%= '/events/editatt/'+event.id%>" class="btn btn-primary">Edit</a>
                        <button class="btn btn-danger" onclick="deleteEventatt()" id="deleteBtn" data-id="<%= event.id%>">Delete</button>
                    <%} else if((user)&&(user.admin!= "true")&&(event.status=="Open")) {%>
                    <button class="btn btn-success btn-margin mt-2" onclick="participate()" data-id="<%= event.id%>" id="part">Participate</button>
                        <%}%>
                </div>
            </div>
        </div>
        
       
    </div>
    
    </div>
    
